<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import java.net.URL?>
<HBox xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="views.controllers.ChatMainPageController"
      prefHeight="720.0" prefWidth="1280.0">

    <VBox fx:id="sidebar" maxWidth="300" styleClass="blueBackground" fillWidth="true">
        <Label text="hello"/>

        <HBox.hgrow>
            <Priority fx:value="ALWAYS">
            </Priority>
        </HBox.hgrow>
    </VBox>

    <VBox fx:id="chatlist" styleClass="greenBackground">
        <HBox.hgrow>
            <Priority fx:value="ALWAYS">
            </Priority>
        </HBox.hgrow>
    </VBox>

    <stylesheets>
        <URL value="@../css/chatroom.css"/>
    </stylesheets>
</HBox>
